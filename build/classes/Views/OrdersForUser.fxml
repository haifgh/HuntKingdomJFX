<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="orders" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.OrdersForUserController">
   <stylesheets>
      <URL value="@shop.css" />
   </stylesheets>
   <children>
      <TableView fx:id="orderstable" layoutX="-7.0" stylesheets="@shop.css">
         <columns>
            <TableColumn fx:id="date" maxWidth="230.0" minWidth="-1.0" prefWidth="230.0" text="Date" />
            <TableColumn fx:id="deliveredAt" maxWidth="230.0" minWidth="-1.0" prefWidth="230.0" text="Delivered at" />
            <TableColumn fx:id="status" maxWidth="284.0" minWidth="-1.0" prefWidth="150.0" text="Status" />
            <TableColumn fx:id="address" maxWidth="580.0" minWidth="-1.0" prefWidth="300.0" text="Address" />
            <TableColumn fx:id="price" maxWidth="985.0" minWidth="-1.0" prefWidth="150.0" text="Price" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
