            <JFXButton fx:id="update_btn" alignment="BASELINE_CENTER" buttonType="RAISED" onAction="#UpdateProduct" prefHeight="32.0" prefWidth="153.0" style="-fx-background-color: #BB86FC;" text="Modify">
