<?import javafx.scene.text.Font?>

<JFXTabPane fx:id="tabPane" prefHeight="602.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SlidePaneAdminController">
