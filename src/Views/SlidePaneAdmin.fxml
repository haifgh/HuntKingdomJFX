                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </JFXButton>
                        <JFXButton buttonType="RAISED" layoutX="10.0" layoutY="304.0" onAction="#goToEvents" prefWidth="300.0" style="-fx-background-color: #013220;" text="EVENTS" textFill="#b5b4b4">
